<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #D1FFBD;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zenbrowser.a1.Controller.HistoryController">
    <top>
        <Label fx:id="greetingLabel" text="Zenbrowser history" BorderPane.alignment="BOTTOM_LEFT">
            <BorderPane.margin>
                <Insets left="10.0" top="10.0" />
            </BorderPane.margin>
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
    </top>
   <center>
      <VBox fx:id="historyContainer" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #edfee2;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
